<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title><%=title%></title>
	</head>
	<body>
		<% extend('layout') %>
		<h1><%=title%></h1>
		<%if(genre_list.length){%>
		<ul>
			<%genre_list.forEach(function(genre){%>
			<li>
				<a href="<%=genre.url%>"> <%=genre.name%> </a>
			</li>
			<%})%>
		</ul>
		<%}else{%>
		<p>There are no genres.</p>
		<%}%>
	</body>
</html>
